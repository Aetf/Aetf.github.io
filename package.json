{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "6.3.0"
  },
  "scripts": {
    "stable-upd-time": "tools/stable_update_time.sh",
    "clean": "hexo clean --silent",
