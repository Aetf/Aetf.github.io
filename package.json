    "test": "ava"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-clean-css": "^2.0.0",
    "hexo-deployer-rsync": "^2.0.0",
    "hexo-excerpt": "^1.1.6",
    "hexo-filter-responsive-images": "^1.7.0",
    "hexo-fs": "^4.1.1",
    "hexo-generator-alias": "^1.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-searchdb": "^1.3.3",
    "hexo-generator-tag": "^2.0.0",
    "hexo-next-publist": "^2.2.1",
    "hexo-prism-plus": "^2.2.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-markdown-it": "^7.1.0",
    "hexo-renderer-stylus": "^3.0.0",
    "hexo-theme-next": "^8.8.1",
    "katex": "^0.13.13",
    "lodash": "^4.17.21",
